generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model MenuItem {
  id               Int    @id @default(autoincrement())
  tipo             String
  nome             String
  descricao        String?
  valor            Float?
  valorPromocional Float?
  tipoComplemento  String?
  qtdMinima        Int?
  qtdMaxima        Int?
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

